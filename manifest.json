{
  "manifest_version": 2,
  "name": "Google Search Console Bulk URL Removal",
  "description": "Google Search Console Bulk URL Removal",
  "version": "1.0",
  "default_locale": "en",

  "browser_action": {
   "default_icon": "icons/icon.png",
   "default_popup": "popup.html"
  },
  "permissions": [
   "activeTab"
 ],
 "content_scripts": [
    {
     "matches": [
       "<all_urls>"
     ],
     "js": ["jquery.min.js", "content.js"]
    }
  ]
}